{
    "name": "Collect",
    "description": "collect some contents",
    "version": "0.1.0",
    "icons": {  "16": "/icon/icon16.png",
                "48": "/icon/icon48.png",
                "128": "/icon/icon128.png" },

    "permissions": ["<all_urls>", "contextMenus", "tabs", "activeTab", "downloads"],


    "commands": {
        "save" : {
            "suggested_key": {
                 "default": "Alt+Shift+S" 
            },
            "description": "Save a link"
        }
    },
    "browser_action": {
        "default_popup": "popup/index.html"
    },
    "background": {
        "scripts": ["jquery-3.4.1.min.js", "background.js"]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["jquery-3.4.1.min.js", "content.js"]
    }],
    "manifest_version": 2
}