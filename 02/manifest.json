{
    "name": "test-extension2",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js":      ["content.js"]
        }
    ],
    "browser_action": {
        // "default_popup": "popup.html",
        "default_title": "Panda"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "*://*/*", "https://stackoverflow.com/*"]
}
